  gem "rspec"
  gem "guard"
  gem "guard-rspec"
  gem 'spork', '> 0.9.0.rc'
  gem 'guard-spork'
  gem 'rb-fsevent'
  gem 'webmock'
  gem 'rake'
  gem 'mock_redis'
end

group :development do
  gem "rbeautify"
end